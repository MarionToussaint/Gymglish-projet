<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_user_area"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/gg"
    tools:context="com.example.mmtou.myapplication.UserAreaActivity">

    <TextView
        android:text="DelavigneCorp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Delavigne"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textColor="@android:color/holo_red_light"
        android:layout_below="@+id/CPF"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp" />

    <TextView
        android:text="Frantastique"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Frantastique"
        android:textStyle="normal|bold"
        android:textSize="16sp"
        android:textColor="@android:color/holo_red_light"
        android:layout_below="@+id/verbe"
        android:layout_alignRight="@+id/CPF"
        android:layout_alignEnd="@+id/CPF"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp" />

    <TextView
        android:text="CPF Anglais"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/CPF"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textColor="@android:color/holo_red_light"
        android:layout_below="@+id/Frantastique"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp" />

    <Button
        android:text="LOGOUT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Logout"
        android:textStyle="normal|bold"
        android:textColor="@android:color/holo_red_light"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="The Word of the Month"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/The_word"
        android:textColor="@android:color/holo_red_light"
        android:textSize="16sp"
        android:textStyle="normal|bold"
        android:layout_below="@+id/Rich"
        android:layout_centerHorizontal="true"
